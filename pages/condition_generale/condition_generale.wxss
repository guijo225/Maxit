.container {
    padding: 20rpx;
    background-color: #ffffff;
  }
  
  .header {
    margin-bottom: 20rpx;
  }
  
  .title {
    font-size: 36rpx;
    font-weight: bold;
    color: #333333;
  }
  
  .subtitle {
    margin-top: 30rpx;
    font-size: 30rpx;
